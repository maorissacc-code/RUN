{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}